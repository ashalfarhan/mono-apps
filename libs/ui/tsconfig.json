{
  "extends": "../../tsconfig.json",
  "compilerOptions": {
    "jsx": "react-jsx",
    "module": "CommonJS",
    "target": "ES2015",
    "moduleResolution": "Node",
    "plugins": [{ "name": "typescript-plugin-css-modules" }]
  },
  "include": ["src/**/*.ts", "src/**/*.tsx", "react-shim.ts"],
  "exclude": ["node_modules"]
}
